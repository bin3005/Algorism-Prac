import java.util.Arrays;
import java.util.Scanner;

public class Main {
	static int[] kLan;
	static int N;
	static long max = 0;
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		int K = input.nextInt();
		N = input.nextInt();
		
		kLan = new int[K];
		for(int i = 0; i < K; i++) {
			kLan[i] = input.nextInt();
		}
		
		Arrays.sort(kLan);
		binarySearch(1, kLan[kLan.length-1]); // 주어진 길이 중 가장 긴 선
		
		System.out.println(max);
		input.close();
	}
	
	public static void binarySearch(long start, long end) {
		long mid = (start+end) / 2;
		
//		System.out.println(start + " " + mid + " " + end);
		long cut = 0;
		
		for(int i = 0; i < kLan.length; i++) {
			cut += kLan[i] / mid;
		}
		
		if(start <= end) {
			if(cut < N) {		// 길이를 줄이자
				binarySearch(start, mid-1);
			}else {				// 길이를 늘리자
				if(mid > max) {
					max = mid;
				}
				binarySearch(mid+1, end);
			}
		}
	}
}
