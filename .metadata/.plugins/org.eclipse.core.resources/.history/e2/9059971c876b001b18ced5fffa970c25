import java.util.ArrayList;
import java.util.Collections;
import java.util.Scanner;

public class Greedy1202 {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int cubeNum = in.nextInt();
		int bagNum = in.nextInt();
		ArrayList<Cube> cubeList = new ArrayList<>();
		
		for(int i = 0 ; i < cubeNum ; i++) {
			cubeList.add(new Cube(in.nextInt(),in.nextInt()));
		}
		
		System.out.println(cubeList);
		
		Collections.sort(cubeList);
		System.out.println(cubeList);
	}

}

class Cube implements Comparable<Cube>{
	int weight;
	int price;
	public Cube() {
		super();
	}
	public Cube(int weight, int price) {
		super();
		this.weight = weight;
		this.price = price;
	}
	@Override
	public int compareTo(Cube cube) {

		if(price < cube.price) {
			return 1;
		}else {
			return -1;
		}
	}
	@Override
	public String toString() {
		return "Cube [weight=" + weight + ", price=" + price + "]";
	}
	
	
	
	
	
}
