import java.util.ArrayList;
import java.util.Scanner;

public class Greedy2839 {

	public static void main(String[] args) {
		//N키로그램 설탕 배달
		//봉지는 3키로와 5키로
		// 3 <= N <= 5000
		Scanner in = new Scanner(System.in);
		
		int smallSugar = 3;
		int bigSugar = 5;
		int boxNum = 0;
		int n = in.nextInt();
		
		if( n % smallSugar == 0) {
			boxNum = n / smallSugar;
		}
		
		if( n % bigSugar == 0) {
			if( boxNum > (n / bigSugar)) {
				boxNum = (n/bigSugar);
			}
		}
		
		if( (n % bigSugar) % smallSugar == 0   ) {
			if( boxNum > ( (n/bigSugar) +( ( n % bigSugar)/smallSugar ) ) ){
				boxNum = (n/bigSugar) +( ( n % bigSugar)/smallSugar );
			}
		}
		
		if(boxNum == 0) {
			boxNum = -1;
		}
		
		System.out.println(boxNum);
	}

}
