import java.util.ArrayList;
import java.util.Collections;
import java.util.Scanner;

public class BinarySearch10316 {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int cardNum = in.nextInt();
		ArrayList<Integer> cardList = new ArrayList();
		for(int i = 0; i < cardNum;i++) {
			cardList.add(in.nextInt());
		}
		int resultNum = in.nextInt();
		ArrayList<Integer> resultList = new ArrayList();
		for(int i = 0; i < resultNum;i++) {
			resultList.add(in.nextInt());
		}
		
		Collections.sort(cardList);
		boolean find = false;
		int mid = 0;
		int cnt = 0;
		for(int i = 0 ; i < resultList.size();i++) {
			find = false;
			int start = 0;
			int end = cardList.size();
			cnt = 0;
			while ( start <= end ) {
				mid = (start+end)/ 2;
				if(mid >= cardList.size()) {
					break;
				}
				
				if( cardList.get(mid) < resultList.get(i) ) {
					start = mid + 1;
				}else {
					end = mid -1;
				}
				if( cardList.get(mid) == resultList.get(i)) {
					find = true;
					
					int result = mid;
					while( cardList.get(result) == resultList.get(i)) {
						cnt++;
						result--;
					}
					
					result = mid;
					while( cardList.get(result) == resultList.get(i)) {
						cnt++;
						result++;
					}
				}
				if(find == true) {
					break;
				}
			}
			System.out.println(cnt);
		}
		
		
	}

}
